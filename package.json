{
  "name": "colist",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "next build --webpack",
    "db:generate-types": "supabase gen types typescript --schema public --local > lib/supabase/database-types.ts && prettier -w lib/supabase/database-types.ts",
    "db:reset": "supabase db reset",
    "dev": "next dev -p 1000",
    "format": "prettier --write .",
    "check-unused": "knip",
    "lint": "eslint .",
    "start": "next start",
    "check-types": "next typegen && tsc --noEmit",
    "check": "bun run lint && bun run check-types && bun run format && bun run db:test"
